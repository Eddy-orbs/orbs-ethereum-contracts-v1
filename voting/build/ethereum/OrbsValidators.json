  "devdoc": {
    "methods": {
      "approve(address)": {
        "details": "Adds a validator to participate in network",
        "params": {
          "validator": "address The address of the validators."
        }
      },
      "constructor": {
        "details": "Constructor that initializes the validators smart contract with the validators metadata registration     database smart contract.",
        "params": {
          "registry_": "IOrbsValidatorsRegistry The address of the validators metadata registration database.",
          "validatorsLimit_": "uint Maximum number of validators list maximum size."
        }
      },
      "getApprovalBlockNumber(address)": {
        "details": "returns the block number in which the validator was approved.",
        "params": {
          "validator": "address The address of the validators."
        }
      },
      "getNetworkTopology()": {
        "details": "returns an array of pairs with node addresses and ip addresses."
      },
      "getValidators()": {
        "details": "returns a list of all validators that have been approved and exist in the validator registration database."
      },
      "getValidatorsBytes20()": {
        "details": "returns a list of all validators that have been approved and exist in the validator registration      database like getValidators but returns byte20 which is more compatible in some cases."
      },
      "isApproved(address)": {
        "details": "returns if an address belongs to the approved list",
        "params": {
          "validator": "address The address of the validators."
        }
      },
      "isOwner()": {
        "return": "true if `msg.sender` is the owner of the contract."
      },
      "isValidator(address)": {
        "details": "returns if an address belongs to the approved list & exists in the validators metadata registration database.",
        "params": {
          "validator": "address The address of the validators."
        }
      },
      "owner()": {
        "return": "the address of the owner."
      },
      "remove(address)": {
        "details": "Remove a validator from the List based on Guardians votes.",
        "params": {
          "validator": "address The address of the validators."
        }
      },
      "renounceOwnership()": {
        "details": "Allows the current owner to relinquish control of the contract."
      },
      "transferOwnership(address)": {
        "details": "Allows the current owner to transfer control of the contract to a newOwner.",
        "params": {
          "newOwner": "The address to transfer ownership to."
        }
      }
    }
  },
  "userdoc": {
    "methods": {
      "renounceOwnership()": {
        "notice": "Renouncing to ownership will leave the contract without an owner. It will not be possible to call the functions with the `onlyOwner` modifier anymore."
      }
    }
  }
