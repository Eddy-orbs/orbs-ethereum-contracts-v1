    "methods": {
      "stakeChange(address,uint256,bool,uint256)": {
        "details": "Notifies of stake change event.",
        "params": {
          "_amount": "uint256 The difference in the total staked amount.",
          "_sign": "bool The sign of the added (true) or subtracted (false) amount.",
          "_stakeOwner": "address The address of the subject stake owner.",
          "_updatedStake": "uint256 The updated total staked amount."
        }
      },
      "stakeChangeBatch(address[],uint256[],bool[],uint256[])": {
        "details": "Notifies of multiple stake change events.",
        "params": {
          "_amounts": "uint256[] The differences in total staked amounts.",
          "_signs": "bool[] The signs of the added (true) or subtracted (false) amounts.",
          "_stakeOwners": "address[] The addresses of subject stake owners.",
          "_updatedStakes": "uint256[] The updated total staked amounts."
        }
      },
      "stakeMigration(address,uint256)": {
        "details": "Notifies of stake migration event.",
        "params": {
          "_amount": "uint256 The migrated amount.",
          "_stakeOwner": "address The address of the subject stake owner."
        }
      }
    },
    "title": "An interface for notifying of stake change events (e.g., stake, unstake, partial unstake, restate, etc.)."
